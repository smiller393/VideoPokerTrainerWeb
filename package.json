{
  "name": "vp-trainer-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "homepage": "https://smiller393.github.io/VideoPokerTrainerWeb",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
